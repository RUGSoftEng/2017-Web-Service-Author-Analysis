{
  "name": "webserver",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "tsc && mocha",
    "build-frontend:windows": "cd ../frontend && elm-make --yes src/Main.elm --output=index.js && move index.js ../backend/public_html/",
    "build-frontend:linux": "cd ../frontend && elm-make --yes src/Main.elm --output=index.js && mv index.js ../backend/public_html/",
    "build-frontend-debug:linux": "cd ../frontend && elm-make --debug src/Main.elm --output=index.js && mv index.js ../backend/public_html/",
    "build:windows": "npm run build-frontend:windows && nodemon --exec \"node_modules\\.bin\\ts-node\" -- \"index.ts\"",
    "build:linux": "npm run build-frontend:linux && nodemon --exec \"node_modules/.bin/ts-node\" -- \"index.ts\"",
    "build:linux-debug": "npm run build-frontend-debug && nodemon --exec \"node_modules/.bin/ts-node\" -- \"index.ts\"",
    "deploy:linux": "\"node_modules/.bin/ts-node\" \"index.ts\""
  },
